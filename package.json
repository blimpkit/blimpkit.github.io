{
  "name": "@blimpkit/blimpkit",
  "version": "1.8.4",
  "description": "BlimpKit is a UI component library",
  "main": "dist/blimpkit.min.js",
  "unpkg": "dist/blimpkit.min.js",
  "scripts": {
    "build": "node ./scripts/build.js",
    "docs:install": "cd docs ; npm i ; cd ..",
    "docs:build": "node ./scripts/build-docs.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html,yaml}\" --config ./.prettierrc",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html,yaml}\" --config ./.prettierrc",
    "start": "npx http-server ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blimpkit/blimpkit.github.io.git"
  },
  "keywords": [
    "blimpkit",
    "ui",
    "widget",
    "components"
  ],
  "contributors": [
    {
      "name": "Stan Genchev",
      "email": "stan.genchev@codbex.com",
      "url": "https://github.com/StanZGenchev"
    },
    {
      "name": "Atanas Himchev",
      "email": "atanas.himchev@codbex.com",
      "url": "https://github.com/himchev"
    }
  ],
  "license": "EPL-2.0",
  "bugs": {
    "url": "https://github.com/blimpkit/blimpkit.github.io/issues"
  },
  "homepage": "https://github.com/blimpkit/blimpkit.github.io#readme",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "clean-css": "^5.3.3",
    "eslint": "^9.13.0",
    "glob": "^11.0.0",
    "globals": "^15.11.0",
    "http-server": "^14.1.1",
    "terser": "^5.34.1",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0"
  },
  "peerDependencies": {
    "@sap-theming/theming-base-content": "11.18.2",
    "angular": "^1.8.2",
    "angular-aria": "^1.8.2",
    "fundamental-styles": "0.38.0",
    "jquery": "3.7.1"
  }
}
